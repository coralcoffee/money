{
  "compilerOptions": {
    "target": "ES2022",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "useDefineForClassFields": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": []
  }
}
